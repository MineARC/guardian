.row
  .col-sm-12
    .panel.panel-default
      .panel-heading
        | TEMPERATURE
      .panel-body
        .row.text-center
          if(aura)
            .col-sm-3
              .inline#readout-temp-aura(data-toggle="modal" data-target="#aura_1_modal")
                div Internal Apparent
                .easyPieChart(ui-jq='easyPieChart', ui-options="{\
                animate: 1000,\
                percent: 0,\
                lineWidth: 5,\
                trackColor: '#b7b7b7',\
                barColor: '#1b82ce',\
                scaleColor: '#ffffff',\
                size: 115,\
                lineCap: 'butt'\
                }", style='width: 115px; height: 115px; line-height: 115px;')
                  div
                    span.h2.value
                      | 0.0
                      span(style='font-size: 0.5em') ℃
                    .text.text-sm.delta
                      i.fa.fa-fw.fa-circle.text-muted.text-sm
                      | 0.0
            .col-sm-3
              .inline#readout-temp-internal(data-toggle="modal" data-target="#temp_1_modal")
                div Internal Actual
                .easyPieChart(ui-jq='easyPieChart', ui-options="{\
