if(series3.system_information)
  div.col-xs-12.col-md-6#table-system-info.items
    h3 System Information
    a.right#monitor-link(href='/monitor') Info
    div.spacer
    table.table
      //- tr
      //-   th Name
      //-   th.table-right Value
      each row in series3.system_information
        tr
          td.row-name= row.row_name
          td.table-right.row-info #{row.row_info} #{row.row_unit}

