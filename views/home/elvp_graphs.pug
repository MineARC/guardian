if(aura)
  .row
    .col-sm-12
      .panel.panel-default
        .panel-heading
          | TEMPERATURA
        .panel-body
          .row.text-center
            .col-sm-12
              .item
                .pos-rlt
                  .item-overlay.bg-black-opacity.r.r-2x.r.r-2x(style='z-index: 10;')
                    .center.text-center.m-t-n.w-full
                      a(data-toggle="modal" data-target="#aura_1_modal")
                        i.fa.fa-3x.fa-line-chart.text-white
                  .inline#readout-temp-aura
                    div Aparente Interna
                    .easyPieChart(ui-jq='easyPieChart', ui-options="{\
                    animate: 1000,\
                    percent: 0,\
                    lineWidth: 5,\
                    trackColor: '#b7b7b7',\
                    barColor: '#1b82ce',\
                    scaleColor: '#ffffff',\
                    size: 115,\
                    lineCap: 'butt'\
                    }", style='width: 115px; height: 115px; line-height: 115px;')
                      case localize
                        when 'us'
                          div
                            span.h2.value
                              | 0.0
                              span(style='font-size: 0.5em') °F
                            .text.text-sm.delta
                              i.fa.fa-fw.fa-circle.text-muted.text-sm
                              | 0.0
                        default
                          div
                            span.h2.value
                              | 0.0
                              span(style='font-size: 0.5em') °C
                            .text.text-sm.delta
                              i.fa.fa-fw.fa-circle.text-muted.text-sm
                              | 0.0
.row
  .col-sm-9
    .panel.panel-default
      .panel-heading
        | VOLTAJE
      .panel-body
        .row.text-center
          .col-sm-3
            .item
              .pos-rlt
                .inline#readout-voltage-mains
                  div Energía Eléctr.
                  span.h2.value
                    | No
          .col-sm-3
            .item
              .pos-rlt
                .inlinee#readout-voltage-inverter
                  div Inversor
                  span.h2.value
                    | No
          .col-sm-3
            .item
              .pos-rlt
                .item-overlay.bg-black-opacity.r.r-2x.r.r-2x(style='z-index: 10;')
                  .center.text-center.m-t-n.w-full
                    a(data-toggle="modal" data-target="#voltage_1_modal")
                      i.fa.fa-3x.fa-line-chart.text-white
                .inlinee#readout-voltage-emergency
                  div Emergency Battery
                  span.h2.value
                    | 0
                    span(style='font-size: 0.5em') V
                  .text-sm.delta
                    i.fa.fa-fw.fa-circle.text-muted.text-sm
                    | 0
          .col-sm-3
            .item
              .pos-rlt
                .item-overlay.bg-black-opacity.r.r-2x.r.r-2x(style='z-index: 10;')
                  .center.text-center.m-t-n.w-full
                    a(data-toggle="modal" data-target="#voltage_2_modal")
                      i.fa.fa-3x.fa-line-chart.text-white
                .inlinee#readout-voltage-standby
                  div Standby Battery
                  span.h2.value
                    | 0
                    span(style='font-size: 0.5em') V
                  .text-sm.delta
                    i.fa.fa-fw.fa-circle.text-muted.text-sm
                    | 0
  .col-sm-3
    .panel.panel-default
      .panel-heading
        | Corriente
      .panel-body
        .row.text-center
          .col-sm-12
            .item
              .pos-rlt
                .item-overlay.bg-black-opacity.r.r-2x.r.r-2x(style='z-index: 10;')
                  .center.text-center.m-t-n.w-full
                    a(data-toggle="modal" data-target="#current_1_modal")
                      i.fa.fa-3x.fa-line-chart.text-white
                .inlinee#readout-current-battery
                  div Batería
                  span.h2.value
                    | 0
                    span(style='font-size: 0.5em') A
                  .text-sm.delta
                    i.fa.fa-fw.fa-circle.text-muted.text-sm
                    | 0