.row
  div.col-xs-12.items
    table.table#table-aura
      tr
        td.row-name Version
        td.table-right.row-info= aura_version
      tr
        th Gas
        th Value
        th Calibration
        th Manufacture
      each gas, key in aura
        if(gas.isRecent)
          tr(class=key)
            td.row-name= key
            td.table-right.row-info #{gas.value} #{aura_params[key].unit}
            td.table-right.row-cal #{gas.cal}
            td.table-right.row-man #{gas.man} 
